{
  "name": "spryker/translator-extension",
  "type": "library",
  "description": "TranslatorExtension module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*",
    "spryker/translator": "*"
  },
  "suggest": {
    "spryker/translator": "If you want to use TranslatorPlugin."
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
